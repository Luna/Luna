from Standard.Base import all

## Type describing a Google Analytics account.
type Google_Analytics_Dimension
    private Value java_record

    ## Dimension API Name.
    apiName : Text
    apiName self = self.java_record.apiName

    ## Dimension display name.
    name : Text
    name self = self.java_record.displayName

    ## Dimension category.
    category : Text
    category self = self.java_record.category

    ## Dimension description.
    description : Text
    description self = self.java_record.description

    ## PRIVATE
    to_text : Text
    to_text self = self.apiName + " (Dimension)"

    ## PRIVATE
    to_display_text : Text
    to_display_text self = "GA Dimension { " + self.apiName + " (" + self.category + ")}"
