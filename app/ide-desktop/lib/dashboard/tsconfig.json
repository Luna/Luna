{
  "extends": "../../tsconfig.json",
  "include": [
    "../types",
    ".",
    "./**/*.json",
    "../../utils.ts",
    ".prettierrc.cjs",
    "../common/src/env.js",
    "../common/src/appConfig.js"
  ],
  "exclude": ["./dist"],
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "outDir": "../../../../node_modules/.cache/tsc",
    "paths": { "#/*": ["./src/*"] },
    "plugins": [
      {
        "name": "ts-plugin-namespace-auto-import"
      }
    ]
  }
}
