    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.1",
    "@types/mapbox-gl": "^2.7.13",
    "@types/node": "^18.17.5",
    "@types/ws": "^8.5.5",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-vue": "^4.3.1",
    "@volar/vue-typescript": "^1.6.5",
    "@vue/eslint-config-prettier": "^8.0.0",
