    "sucrase": "^3.34.0",
    "vue": "^3.3.4",
    "ws": "^8.13.0",
    "y-protocols": "^1.0.5",
    "y-textarea": "^1.0.0",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.7"
