    "isomorphic-ws": "^5.0.0",
    "lib0": "^0.2.85",
    "sha3": "^2.1.4",
    "magic-string": "^0.30.3",
    "pinia": "^2.1.6",
    "postcss-inline-svg": "^6.0.0",
