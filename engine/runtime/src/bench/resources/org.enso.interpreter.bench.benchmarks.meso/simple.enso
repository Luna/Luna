from Standard.Base import all
import Standard.Test.Bench.Bench

options = Bench.options . size 10 . iter 5

vec = [1,2,3,4,5]

all = Bench.build builder->
    builder.group "group_1" options group_builder->
        group_builder.specify "bench_1" <|
            vec + [10]
    builder.group "group_2" options group_builder->
        group_builder.specify "bench_2" <|
            vec + [20]

main =
    all.run_main


