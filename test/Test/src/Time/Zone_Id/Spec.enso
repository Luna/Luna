from Base import all

import Base.Test
import Base.Time.Zone_Id

spec =
    describe "Zone_Id" <|
        it "should get system zone id" <|
            Zone_Id.system
        it "should parse zone id" <|
            zone = "UTC"
            id = Zone_Id.parse zone
            id . get_id . should_equal zone
        it "should get utc zone id" <|
            id = Zone_Id.utc
            id . get_id . should_equal "UTC"
